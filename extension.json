{
	"name": "ComplimentTuesday",
	"author": [
		"Jakel181"
	],
	"url": "https://github.com/jakel181/Compliment-Tuesday",
	"descriptionmsg": "ComplimentTuesday-desc",
	"license-name": "GPLv3",
	"type": "specialpage",
	"AutoloadClasses": {
		"SpecialComplimentTuesday": "SpecialComplimentTuesday.php",
		"SpecialViewCompliments": "SpecialViewCompliments.php"
    },
    "config": {
		"ManageCompliments": sysop
	},
	"AvailableRights": [
		"compliment",
		"view-compliments"
	],
	"SpecialPages": {
		"ComplimentTuesday": "SpecialComplimentTuesday",
		"ViewCompliments": "SpecialViewCompliments"
	},
	"GroupPermissions": {
		"user": {
			"compliment": true
		},
		"*": {
			"compliment": false,
			"view-compliments": false
		},
		"$wgManageCompliments": {
			"view-compliments": true
		}
	},
	"ResourceModules": {
		"ext.report": {
			"styles": "main.css",
			"localBasePath": "resources",
			"remoteExtPath": "Report/resources"
		}
	},
	"MessagesDirs": {
		"Report": [
			"i18n"
		]
	},
	"manifest_version": 1
}
